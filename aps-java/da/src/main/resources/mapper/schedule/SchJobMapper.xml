<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aps.mapper.schedule.SchJobMapper">

    <!-- 基础结果映射 -->
    <resultMap id="BaseResultMap" type="com.aps.entity.schedule.SchJob">
        <id column="id" property="id"/>
        <result column="job_no" property="jobNo"/>
        <result column="horizon_start" property="horizonStart"/>
        <result column="horizon_end" property="horizonEnd"/>
        <result column="scope_json" property="scopeJson" typeHandler="com.baomidou.mybatisplus.extension.handlers.JacksonTypeHandler"/>
        <result column="objective_json" property="objectiveJson" typeHandler="com.baomidou.mybatisplus.extension.handlers.JacksonTypeHandler"/>
        <result column="constraint_json" property="constraintJson" typeHandler="com.baomidou.mybatisplus.extension.handlers.JacksonTypeHandler"/>
        <result column="status" property="status"/>
        <result column="engine_trace" property="engineTrace"/>
        <result column="error_msg" property="errorMsg"/>
        <result column="created_time" property="createdTime"/>
        <result column="created_by" property="createdBy"/>
        <result column="updated_time" property="updatedTime"/>
        <result column="updated_by" property="updatedBy"/>
        <result column="deleted" property="deleted"/>
    </resultMap>

    <!-- 基础列 -->
    <sql id="Base_Column_List">
        id, job_no, horizon_start, horizon_end, scope_json, objective_json, constraint_json,
        status, engine_trace, error_msg,
        created_time, created_by, updated_time, updated_by, deleted
    </sql>

</mapper>

